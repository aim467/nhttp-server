    "compression": "^1.8.1",
    "cookie-parser": "^1.4.7",
    "cors": "^2.8.6",
    "ejs": "^3.1.10",
    "express": "^5.2.1",
    "mime": "^3.0.0",
    "morgan": "^1.10.1"
  },
  "engines": {
    "node": ">=16.0.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/aim467/nhttp-server.git"
  },
  "bugs": {
